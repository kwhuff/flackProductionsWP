{
  "extends": ["eslint:recommended", "google"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
  ],
  "rules": {
    "max-len": ["error", { "code": 120 }]
  }
}
